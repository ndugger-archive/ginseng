{
    "presets": [ "stage-0", "react" ],
    "plugins": [
        [ "transform-es2015-modules-commonjs" ],
        [ "transform-async-to-module-method", { "module": "bluebird", "method": "coroutine" } ],
        [ "transform-regenerator", { "asyncGenerators": false, "generators": false, "async": false } ]
    ]
}
